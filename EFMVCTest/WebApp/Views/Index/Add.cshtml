@using EFModel;
@model EFModel.NewTypes
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add</title>
</head>
<body>
    <div> 
        @using (Html.BeginForm("Add", "post", FormMethod.Post))
        {
        @Html.TextBoxFor(u => u.TypeId)
        <br />
        @Html.TextBoxFor(u => u.TypeConent)
        <br />
        @Html.DropDownListFor(u => u.NewsId,(IEnumerable<SelectListItem>) ViewBag.typelist)
            <br />
        <input type="submit" value="添加" />
        }
        
    </div>
</body>
</html>
